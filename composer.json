            "url": "https://get.spip.net/composer/"
        }
    },
    "autoload": {
        "psr-4": {
            "SpipRemix\\Polyfill\\Meta\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpipRemix\\Polyfill\\Meta\\Test\\": "tests/"
        },
        "files": [
            "inc/meta.php"
